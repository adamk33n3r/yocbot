{
  "name": "discordbot",
  "version": "1.1.0",
  "description": "",
  "main": "dist/src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV='prod' NODE_PATH='./dist' node dist/src/index.js",
    "startdev": "cross-env NODE_ENV='dev' ts-node -r tsconfig-paths/register src/index.ts",
    "starttest": "cross-env NODE_ENV='dev' ts-node -r tsconfig-paths/register src/index.ts --testbot",
    "build": "tsc -p tsconfig.json",
    "auth": "ts-node auth-yt.ts",
    "deploy-commands": "ts-node -r tsconfig-paths/register src/deploy-commands.ts",
    "deploy-commands-test": "ts-node -r tsconfig-paths/register src/deploy-commands.ts --testbot",
    "clear-commands-test": "ts-node -r tsconfig-paths/register src/deploy-commands.ts --testbot --clear",
    "start-emulator": "firebase emulators:start --only=firestore",
    "tsm": "ts-node -r tsconfig-paths/register src/TSMAPI.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.15.0",
    "bufferutil": "^4.0.7",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "discord.js": "^14.8.0",
    "ffmpeg-static": "^5.1.0",
    "firebase-admin": "^11.5.0",
    "got": "^12.6.0",
    "keyv": "^4.5.2",
    "keyv-firestore": "github:goto-bus-stop/keyv-firestore",
    "libsodium-wrappers": "^0.7.11",
    "node-cron": "^3.0.2",
    "node-youtube-music": "^0.7.0",
    "play-dl": "^1.9.6",
    "reflect-metadata": "^0.1.13",
    "utf-8-validate": "^5.0.10",
    "yargs": "^17.7.1",
    "yt-search": "^2.10.4"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/node-cron": "^3.0.7",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "chalk": "^4.1.2",
    "eslint": "^8.37.0",
    "firebase-tools": "^11.25.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "winston": "^3.8.2"
  }
}
